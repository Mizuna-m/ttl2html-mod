<dl class="row">
<%- param[:data].sort_by{|k, v|
      order = param[:orders][k] if param[:orders] and param[:orders][k]
      order = Float::INFINITY if order.nil?
      [ order, k ]
    }.each do |k, v| -%>
  <dt class="col-sm-3"><%=h t('triples.blank_node') %></dt>
  <dd class="col-sm-9"><%= format_triples(param[:data]) %></dd>
<%- end -%>
</dl>